{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["snow_profile.js",
                    "snow_profile_button.js",
                    "snow_profile_layer.js",
                    "snow_profile_main.js"
                   ]
    },
    "plugins": [ "plugins/markdown" ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "destination": "./docs",
        "private"    : true
    }
}
